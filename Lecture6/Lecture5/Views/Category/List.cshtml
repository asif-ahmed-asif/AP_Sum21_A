@model List<Lecture5.Models.Category>
@{
    ViewBag.Title = "List";
}

<h2>List</h2>

<div class="container">
    <table class="table table-condensed">
        <tr>
            <th>Id</th>
            <th>Name</th>
            <th></th>
            <th></th>
        </tr>
        @foreach (var c in @Model)
        {
            <tr>
                <td>@c.Id</td>
                <td>@c.Name</td>
                
                <td><a class="btn btn-success" href="">Edit</a></td>
                <td><a class="btn btn-danger">Delete</a></td>
            </tr>
        }

    </table>
</div>